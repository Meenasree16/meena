        "def Temp_modeling(a,b,c,time):\n",
        "  temparature = a*time**2 + b*time +c\n",
        "  return temparature\n",
        "\n",
        "# Hardcoded variables\n",
        "a,b,c=0,1,10\n",
        "time = 5\n",
        "\n",
        "#display results\n",
        "print(\"Hard-coded variables for weather modeling\")\n",
        "print(\"Temparature for hardcoded coefficients at time\",time,\"hours:\",Temp_modeling(a,b,c,time))\n",
        "\n",
        "# Define and assign values to the variables in the same cell where they are used.\n",
        "a, b, c = 0, 1, 10\n",
        "time = 5\n",
        "\n",
        "# Now, you can use the variables without any NameError\n",
        "print(\"Hard-coded variables for weather modeling\")\n",
        "# Call the function with the correct name 'Temp_modeling'\n",
        "print(\"Temparature for hardcoded coefficients at time\", time, \"hours:\", Temp_modeling(a, b, c, time))"
